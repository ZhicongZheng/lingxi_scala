->      /                            api.ApiRouter

GET     /                            controllers.Default.redirect(to = "./docs/")

GET     /users                       auth.controller.UserController.getAll

GET     /users/:userId               auth.controller.UserController.getById(userId: Long)

POST    /admin/login                 auth.controller.AuthWriteController.login

GET     /admin/currentUserInfo       auth.controller.AuthReadController.currentUserInfo